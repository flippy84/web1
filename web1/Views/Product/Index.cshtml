@using web1.Models;
@model List<Product>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <table>
        @foreach (var p in Model)
        {
        <tr>
            <td>@p.Name</td>
            <td>@(p.PurchasePrice * 2.5M)</td>
            <td>@p.Stock</td>
        </tr>
        }
    </table>
</body>
</html>
